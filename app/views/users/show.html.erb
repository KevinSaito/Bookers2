<%= render 'books/list', book: @newbook, user: @user %>

<h1>Books Detail</h1>
<table>
    <thead>
        <th>Title</th>
        <th>Opinion</th>
        <th colspan="3"></th>
    </thead>
    <tbody>
      <% @books.each do |book| %>
      <tr>
        <td><%= image_tag user.get_profile_image("100x100") %></td>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
        
        
      </tr>
      <% end %>
    </tbody>
</table>